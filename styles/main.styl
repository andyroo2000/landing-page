*
  font-family: 'helvetica'
  color: #666

body
  height: 100%
  width: 100%
  padding-left: 3em
  font-size: 1.8em
  line-height: 1.6
  width: 50%
  background: url(../images/me-fire-escape.jpg) no-repeat center center fixed
  

a
  text-decoration: none
  width: 100%
  height: 100%

ul
  list-style-type: none
  padding-left: 0
  margin: 0 0 2em 0

li
  width: 30%
  text-align: center
  cursor: pointer

header
  font-size: 3em
  margin-top: 0.2em
  color: #DDD
  color: RGBA(221, 221, 221, .9)

h2
  margin-top: 0
  font-size: 0.75em
  font-weight: normal
  color: #7f8c8d

.fa
  color: white
  line-height: 2

.mySite
  background: rgba(204, 204, 204, .7)
  font-size: 60%
  height: 1.8em
  vertical-align: middle


.gitHub
  background: rgba(128, 128, 128, .7)

.twitter
  background: rgba(0, 172, 237, .7)

.youtube
  background: rgba(187, 0, 0, .7)

.vimeo
  background: rgba(170, 212, 80, .7)

.stackOverflow
  background: rgba(224, 135, 73, .7)

.fourSquare
  background: rgba(0, 114, 177, .7)

.instagram
  background: rgba(81, 127, 237, .7)

.flickr
  background: rgba(255, 0, 132, .7)


